{
    "makefile.extensionOutputFolder": "./.vscode",
    "files.associations": {
        "ch32v00x_conf.h": "c",
        "ch32v00x_it.h": "c",
        "core_riscv.h": "c",
        "ch32v00x.h": "c",
        "debug.h": "c",
        "string.h": "c",
        "arduino.h": "c",
        "ch32v00x_dma.h": "c",
        "crtdefs.h": "c"
    },
    "cmake.generator": "Unix Makefiles",
    "cmake.configureEnvironment": {
        "CMAKE_EXPORT_COMPILE_COMMANDS": "on"
    },
    "cmake.configureSettings": {
        "CMAKE_MAKE_PROGRAM": "${workspaceFolder}\\..\\tools\\build-utils\\bin\\make.exe"
    },
    "C_Cpp.default.intelliSenseMode": "gcc-arm",
    "C_Cpp.default.configurationProvider": "ms-vscode.cmake-tools", 
    "cortex-debug.openocdPath.windows": "${workspaceFolder}\\..\\tools\\OpenOCD\\bin\\openocd.exe", 
    "cortex-debug.gdbPath.windows": "${workspaceFolder}\\..\\tools\\riscv-embedded-gcc-12\\bin\\riscv-none-elf-gdb.exe",
    "cortex-debug.objdumpPath.windows": "${workspaceFolder}\\..\\tools\\riscv-embedded-gcc-12\\bin\\riscv-none-elf-objdump.exe",
    "cortex-debug.registerUseNaturalFormat": false,
    "cortex-debug.variableUseNaturalFormat": true,
    "C_Cpp.intelliSenseEngine": "default"
}